for _ in range(int(input())):
     n=int(input())
     arr=list(map(int,input().strip().split()))[:n*2]
     odd=0
     even=0
     for i in arr:
          if(i%2==0):
               even+=1
          else:
               odd+=1
 
     if(odd==even):
          print("Yes")
     else:
          print("No")
