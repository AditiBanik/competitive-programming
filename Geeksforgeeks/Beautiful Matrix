#include <iostream>
#include <bits/stdc++.h>
#define ll long long int
using namespace std;

int32_t main(){
    int arr[5][5];
    int px=0;
    int py=0;
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            cin>>arr[i][j];
            if(arr[i][j]==1){
                px=i;
                py=j;   
            }
        }
    }
    int val=abs(2-px)+abs(2-py);
    cout<<val<<endl;
}

