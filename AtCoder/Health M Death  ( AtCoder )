#include <iostream>
#include <bits/stdc++.h>
#include <cmath>
#define ll long long int

using namespace std;

int main()
{
ll n,m;
cin>>n>>m;
if(n==0){
    cout<<"No"<<"\n";
}
else if(m==0){
    cout<<"Yes"<<"\n";
}
else if(m%n==0){
    cout<<"Yes"<<"\n";
}
else{
    cout<<"No"<<"\n";
}
return 0;
}
