#include <iostream>
#define ll long long int
#include <bits/stdc++.h>
using namespace std;

int main()
{
    ll t;
    cin>>t;
    while(t--){
        ll m,h;
        cin>>m>>h;
        h=pow(h,2);
        ll val=m/h;
        if(val<=18){
            cout<<1<<endl;
        }
        else if(val>18 and val<25){
            cout<<2<<endl;
        }
        else if(val>24 and val<30){
            cout<<3<<endl;
        }
        else{
            cout<<4<<endl;
        }
    }
    
   return 0;
}
