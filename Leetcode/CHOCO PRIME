# cook your dish here
def check_prime(n):
    i=2
    while(i*i<=n):
        if(n%i==0):
            return False
        i+=1
    return True

t=int(input())
for i in range(t):
    n=int(input())
    if(check_prime(n)):
        val=str(n)
        val=val[::-1]
        if(int(val)==n):
            print("Yes")
        else:
            print("No")
        
    else:
        print("No")
    
    
        
