try:    
    test=int(input())
    for _ in range(test):
        total_nums=int(input())
        arr_of_data=list(map(int,input().strip().split()))[:total_nums]

        if 0 in set(arr_of_data):
            print(len(set(arr_of_data))-1)
        else:
            print(len(set(arr_of_data)))
    
except:
    pass
