class Solution:
    def bitwiseComplement(self, N: int) -> int:
        binary=str(bin(N))
        binarystr=binary[2:]
        print(binarystr)
        ans=""
        for i in binarystr:
            if(i=='0'):
                ans+='1'
            else:
                ans+='0'
        
        return(int(ans,2))
        
