for t in range(int(input())):
    n,k=map(int,input().strip().split())
    arr=list(map(int,input().strip().split()))[:k]
    for _ in range(0,n):
        count=0
        temp=input()
        for i in range(0,k):
            if(temp[i]=='1'):
                count+=arr[i]

        print(count)
    
        
            
